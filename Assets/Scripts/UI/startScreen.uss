.start-screen-button {
    width: 40%;
    height: 200px;
    background-color: rgb(75, 75, 128);
    border-width: 3px;
    border-color: rgb(255, 215, 0);
    border-radius: 10px;
    margin: 15px 0;
}

.start-screen-button-text {
    font-size: 48px;
    -unity-font-style: bold;
    color: white;
    -unity-text-align: middle-center;
    width: 100%;
    height: 100%;
}
